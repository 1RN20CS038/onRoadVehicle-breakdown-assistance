<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityServicePLogin" >

    <ImageView
        android:id="@+id/imageView48sp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="50dp"
        android:rotation="-90"
        app:srcCompat="@drawable/white" />

    <ImageView
        android:id="@+id/imageView86sp"
        android:layout_width="201dp"
        android:layout_height="408dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="210dp"
        android:layout_marginBottom="-25dp"
        app:srcCompat="@drawable/purpbottom" />

    <ImageView
        android:id="@+id/imageView11sp"
        android:layout_width="179dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-16dp"
        android:layout_marginBottom="393dp"
        app:srcCompat="@drawable/purptop" />

    <ImageView
        android:id="@+id/imageView98sp"
        android:layout_width="334dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="218dp"
        android:layout_marginBottom="574dp"
        android:rotation="-225"
        app:srcCompat="@drawable/optional" />

    <ImageView
        android:id="@+id/iV88sp"
        android:layout_width="227dp"
        android:layout_height="141dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-15dp"
        android:layout_marginBottom="0dp"
        android:rotation="-225"
        app:srcCompat="@drawable/optional" />


    <EditText
        android:id="@+id/loginmailsp"
        android:layout_width="273dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="103dp"
        android:layout_marginTop="245dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="443dp"
        android:background="@drawable/input_bg"
        android:ems="10"
        android:hint="Email"
        android:inputType="text"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/sploginpass"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="102dp"
        android:layout_marginTop="340dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="348dp"
        android:background="@drawable/input_bg"
        android:ems="10"
        android:hint="Password"
        android:inputType="text"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center" />


    <Button
        android:id="@+id/sploginsubmit"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="229dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="222dp"
        android:text="LOGIN"
        app:backgroundTint="#9C27B0" />

    <ImageView
        android:id="@+id/iV25sp"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="434dp"
        app:srcCompat="@drawable/emailnew" />

    <ImageView
        android:id="@+id/imageV28sp"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="303dp"
        android:layout_marginBottom="335dp"
        app:srcCompat="@drawable/passnew" />

    <TextView
        android:id="@+id/textViewsp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="505dp"
        android:text=" SERVICE PROVIDER LOGIN"
        android:textAlignment="center"
        android:textColor="#06030C"
        android:textSize="34sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/serviceloginback"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="229dp"
        android:layout_marginBottom="222dp"
        android:text="BACK"
        app:backgroundTint="#9C27B0" />



</RelativeLayout>